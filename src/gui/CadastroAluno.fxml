<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="cena" prefHeight="480.0" prefWidth="720.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.CadastroAlunoController">
   <children>
      <Label layoutX="286.0" layoutY="143.0" text="Nome:" />
      <TextField fx:id="nome" layoutX="286.0" layoutY="160.0" onMouseClicked="#getSource" />
      <Label layoutX="286.0" layoutY="196.0" text="Login:" />
      <TextField fx:id="login" layoutX="286.0" layoutY="213.0" onMouseClicked="#getSource" />
      <Label layoutX="286.0" layoutY="246.0" text="Senha:" />
      <PasswordField fx:id="senha" layoutX="286.0" layoutY="263.0" onMouseClicked="#getSource" />
      <Label layoutX="320.0" layoutY="117.0" text="Cadastro Aluno" />
      <Button layoutX="285.0" layoutY="308.0" mnemonicParsing="false" onAction="#validar" scaleZ="10.0" text="Cadastrar" />
      <Button layoutX="387.0" layoutY="308.0" mnemonicParsing="false" onAction="#showLogin" text="Voltar" />
   </children>
</AnchorPane>
