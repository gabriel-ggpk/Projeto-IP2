<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="cena" prefHeight="299.0" prefWidth="272.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.CadastroDisciplinaController">
   <children>
      <Label layoutX="95.0" layoutY="63.0" text="Cadastro Disciplina" />
      <Label layoutX="77.0" layoutY="95.0" text="Nome:" />
      <TextField fx:id="nome" layoutX="72.0" layoutY="120.0" onMouseClicked="#getSource" />
      <Label layoutX="72.0" layoutY="150.0" text="Vagas:" />
      <TextField fx:id="vagas" layoutX="71.0" layoutY="168.0" onKeyTyped="#chamaRemover" onMouseClicked="#getSource" />
      <Label layoutX="74.0" layoutY="202.0" text="Total de Aulas:" />
      <TextField fx:id="aulas" layoutX="71.0" layoutY="220.0" onKeyTyped="#chamaRemover" onMouseClicked="#getSource" />
      <Button layoutX="74.0" layoutY="254.0" mnemonicParsing="false" onAction="#validar" scaleZ="10.0" text="Cadastrar" />
      <Button layoutX="162.0" layoutY="254.0" mnemonicParsing="false" onAction="#voltarTela" text="Voltar" />
   </children>
</AnchorPane>
